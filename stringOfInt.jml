let fun stringOfInt(int a) => (String):
    match a%10 with
    | 0     ->    stringOfInt(a/10) + "0"
    | 1     ->    stringOfInt(a/10) + "1"
    | 2     ->    stringOfInt(a/10) + "2"
    | 3     ->    stringOfInt(a/10) + "3"
    | 4     ->    stringOfInt(a/10) + "4"
    | 5     ->    stringOfInt(a/10) + "5"
    | 6     ->    stringOfInt(a/10) + "6"
    | 7     ->    stringOfInt(a/10) + "7"
    | 8     ->    stringOfInt(a/10) + "8"
    | 9     ->    stringOfInt(a/10) + "9"